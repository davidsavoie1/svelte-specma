{
  "name": "svelte-specma",
  "version": "2.0.0",
  "description": "Svelte store for data validation using Specma",
  "homepage": "https://github.com/davidsavoie1/svelte-specma.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidsavoie1/svelte-specma.git"
  },
  "bugs": {
    "url": "https://github.com/davidsavoie1/svelte-specma/issues"
  },
  "keywords": [
    "svelte",
    "store",
    "specma",
    "spec",
    "schema",
    "validation",
    "composition",
    "ensure",
    "conform",
    "validate",
    "predicates"
  ],
  "author": "David Savoie",
  "license": "ISC",
  "source": "src/index.js",
  "main": "dist/svelte-specma.umd.js",
  "module": "dist/svelte-specma.modern.js",
  "exports": {
    ".": {
      "import": "./dist/svelte-specma.modern.js"
    }
  },
  "scripts": {
    "build": "microbundle -f esm,cjs --generateTypes false",
    "dev": "microbundle watch -f esm,cjs --generateTypes false"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "microbundle": "^0.13.0",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "fast-deep-equal": "^3.1.3"
  },
  "peerDependencies": {
    "svelte": ">=3.0.0 <6.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": false
    }
  },
  "microbundle": {
    "external": [
      "svelte/store"
    ]
  },
  "sideEffects": false
}
